<div class="container">
  <div class="page-header">
    <h1>
      <%= @name %>
    </h1>
  </div>
  <table class="table">
    <thead>
      <tr>
        <th>Name</th>
        <th>Estimate</th>
        <th>
          <%= " Labels" %>
        </th>
        <th>Current State</th>
      </tr>
    </thead>
    <tbody></tbody>
    <% @tracker_stories.each do |story| %>
    <tr>
      <td>
        <%= story[:name] %>
      </td>
      <td>
        <%= story[:estimate] %>
      </td>
      <td>
        <% @labels = "" %>
        <% story[:labels].each do |label| %>
        <% @labels += " #{label[:name]}," %>
        <% end %>
        <%= @labels.chop %>
      </td>
      <td>
        <%= story[:current_state] %>
      </td>
    </tr>
    <% end %>
  </table>
</div>
