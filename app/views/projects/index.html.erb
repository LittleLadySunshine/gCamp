<div class="col-md-3">
  <h1>Projects</h1>
</div>


<div class= "container_buttons pull-right">
    <%= link_to 'Create Project', new_project_path, class: "btn btn-info" %>
</div>

<table class="table">


  <thead>
    <tr>
      <th>Name</th>
    </tr>
  </thead>

  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= link_to project.name, project_path(project) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
