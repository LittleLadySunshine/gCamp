<div class="col-md-3">
  <h1>Users</h1>
</div>


<div class= "container_buttons pull-right">
    <%= link_to 'Create User', new_user_path, class: "btn btn-info" %>
</div>

<table class="table">


  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
    </tr>
  </thead>

  <tbody>
    <% @user.each do |user| %>
      <tr>
        <td><%= link_to user.first_name, user_path(user) %>
          <%= link_to user.last_name, user_path(user) %></td>
        <td><%= link_to user.email, user_path(user) %></td>

        <td><%= link_to 'Edit', edit_user_path(user) %></td>


      </tr>
    <% end %>
  </tbody>
</table>
